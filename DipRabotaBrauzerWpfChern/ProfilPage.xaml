<Page x:Class="DipRabotaBrauzerWpfChern.ProfilPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DipRabotaBrauzerWpfChern"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ProfilPage">

    <Grid Background="White">
        <TextBox HorizontalAlignment="Center" Height="35" Margin="0,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="250"/>
        <TextBox HorizontalAlignment="Center" Height="35" Margin="0,180,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="250"/>
        <Label Content="Логин:" HorizontalAlignment="Center" Height="45" Margin="0,55,0,0" VerticalAlignment="Top" Width="250" FontSize="20" VerticalContentAlignment="Center"/>
        <Label Content="Пароль:" HorizontalAlignment="Center" Height="45" Margin="0,135,0,0" VerticalAlignment="Top" Width="250" FontSize="20" VerticalContentAlignment="Center"/>
        <Button Content="Войти" HorizontalAlignment="Center" Height="34" Margin="0,220,0,0" VerticalAlignment="Top" Width="250"/>
    </Grid>
</Page>
